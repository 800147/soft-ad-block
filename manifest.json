{
  "manifest_version": 2,
  "name": "Soft ad block",
  "version": "1.9",
  "description": "Blurs advertisement on the page",
  "icons": {
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "author": "Boolat Kamalov",
  "homepage_url": "https://800147.github.io/soft-ad-block/",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"]
    }
  ]
}
