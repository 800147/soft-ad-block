{
  "manifest_version": 2,
  "name": "Soft ad block",
  "version": "1.3",
  "description": "Blurs advertisement on the page",
  "icons": {
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "author": "Boolat Kamalov",
  "homepage_url": "https://github.com/800147/soft-ad-block",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
